#include <stdio.h>
int main() {
    int product;
    int arr[2][2] ={
            {1,1,},
            {1,1}
    };
    printf("enter number: ");
    scanf("%d", &arr[0][0]);

    printf("enter number: ");
    scanf("%d", &arr[1][0]);

    printf("enter number: ");
    scanf("%d", &arr[0][1]);

    printf("enter number: ");
    scanf("%d", &arr[1][1]);

    product = arr[0][0]*arr[1][1];
    printf("%d\n", product);

    product = arr[1][0]*arr[0][1];
    printf("%d", product);


}
